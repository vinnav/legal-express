extends layout

block content
  h1= title

  form(method='POST' action='' encType='multipart/form-data')
    label(for='file') File:
    input#file.form-control(type='file', name='document' required='true')
    br
    button.btn.btn-primary(type='submit') Submit

  if errors 
    ul
      for error in errors
        li!= error.msg